import java.util.*;
class Joinarraylists
{
public static void main(String args[])
{
ArratList<Integer> a=new ArrayList<Integer>();
a.add(10);
a.add(20);
a.add(30);
a.add(40);
ArrayList<Integer> b=new ArrayList<Integer>();
b.add(100);
b.add(200);
b.add(300);
b.add(400);
a.addAll(b);
System.out.println("Joined ArrayList: ");
System.out.println(a);
}
}
